{
	"name": "sveltekit-meta",
	"version": "0.2.0",
	"description": "SvelteKit metadata utilities",
	"license": "MIT",
	"keywords": [
		"svelte",
		"sveltekit",
		"metadata",
		"seo",
		"utility",
		"meta-tags",
		"open-graph",
		"head",
		"svelte-meta",
		"svelte-seo"
	],
	"author": "notnotjake",
	"repository": {
		"type": "git",
		"url": "https://github.com/notnotjake/sveltekit-meta",
		"directory": "packages/sveltekit-meta"
	},
	"private": false,
	"type": "module",
	"scripts": {
		"build": "svelte-kit sync && svelte-package",
		"prepublishOnly": "npm run build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"main": "dist/index.js",
	"module": "dist/index.js",
	"peerDependencies": {
		"svelte": "^5.0.0",
		"@sveltejs/kit": "^2.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@types/bun": "1.2.18",
		"eslint": "^9.31.0",
		"eslint-plugin-svelte": "^3.10.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.36.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist"
	],
	"svelte": "./dist/index.js",
	"types": "dist/index.d.ts",
	"dependencies": {
		"zod": "^4.0.5"
	}
}
