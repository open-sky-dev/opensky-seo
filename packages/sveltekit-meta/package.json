{
	"author": "notnotjake",
	"description": "SvelteKit metadata utilities",
	"name": "sveltekit-meta",
	"repository": {
		"directory": "packages/sveltekit-meta",
		"type": "git",
		"url": "git+https://github.com/notnotjake/sveltekit-meta.git"
	},
	"license": "MIT",
	"dependencies": {
		"zod": "^4.0.5"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"keywords": [
		"svelte",
		"sveltekit",
		"metadata",
		"seo",
		"utility",
		"meta-tags",
		"open-graph",
		"head",
		"svelte-meta",
		"svelte-seo"
	],
	"peerDependencies": {
		"@sveltejs/kit": "^2.0.0",
		"svelte": "^5.0.0"
	},
	"private": false,
	"scripts": {
		"build": "vite build && npm run prepack",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"prepare": "svelte-kit sync || echo ''",
		"preview": "vite preview"
	},
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"type": "module",
	"types": "./dist/index.d.ts",
	"version": "0.0.1"
}
